concrete InlineRunner<#id> {
  refines TaskRunner<#id>
  @type new () -> (#self)
}

concrete Compute<#id,#x> {
  #id immutable
  #id defines Equals<#id>
  #id requires Hashed

  @category with<#id,#x>
    #id immutable
    #id defines Equals<#id>
    #id requires Hashed
  ([AsyncValue<#x>&AsyncNode<#id>],TaskRunner<#id>) -> (#x)
}
