concrete InlineRunner<#id> {
  refines TaskRunner<#id>

  // Args:
  // - Int: Max concurrent tasks, 0 for unlimited.
  @type new (Int) -> (#self)
}

concrete Compute<#id> {
  @category with<#id,#x>
    #id immutable
    #id defines Equals<#id>
    #id requires Hashed
  ([AsyncValue<#x>&AsyncNode<#id>],TaskRunner<#id>) -> (#x)

  @category allWith<#id>
    #id immutable
    #id defines Equals<#id>
    #id requires Hashed
  (DefaultOrder<AsyncNode<#id>>,TaskRunner<#id>) -> ()
}
